<%- include('../partials/header') %>

All Books

<img src="/images/Starman.jpg" alt="">

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Author</th>
            <th>Release Year</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <% books.forEach(function(b) { %>
            <tr>
                <td><%= b.title %></td>
                <td><%= b.author %></td>
                <td><%= b.releaseYear %></td>
                <td><%= b.genre %></td>
            </tr>
        <% }); %>
    </tbody>
</table>

<%- include('../partials/footer') %>